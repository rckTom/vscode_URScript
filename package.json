{
    "name": "urscript",
    "displayName": "urscript",
    "description": "Basic language support for the Universal Robot Script Language",
    "version": "1.0.0",
    "publisher": "rckTom",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "urscript",
            "aliases": ["URScript", "urscript"],
            "extensions": [".script",".urscript"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "urscript",
            "scopeName": "source.urscript",
            "path": "./syntaxes/urscript.tmLanguage.json"
        }]
    }
}